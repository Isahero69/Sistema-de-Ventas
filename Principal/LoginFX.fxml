<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Principal.LoginFXController">
   <children>
      <Label alignment="CENTER" layoutX="16.0" layoutY="23.0" prefHeight="46.0" prefWidth="359.0" text="¡BIENVENIDO!">
         <font>
            <Font name="Century Gothic Bold" size="36.0" />
         </font></Label>
      <TextField fx:id="txtIdentificacionLogin" layoutX="52.0" layoutY="147.0" onAction="#enterID" onKeyTyped="#validar" prefHeight="35.0" prefWidth="280.0" promptText="Ingrese su identificacion" />
      <Label layoutX="52.0" layoutY="114.0" prefHeight="17.0" prefWidth="132.0" text="Identificacion:">
         <font>
            <Font name="Century Gothic Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="52.0" layoutY="214.0" prefHeight="17.0" prefWidth="132.0" text="Contraseña:">
         <font>
            <Font name="Century Gothic Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnLogin" layoutX="106.0" layoutY="320.0" mnemonicParsing="false" onAction="#actionEvent" prefHeight="46.0" prefWidth="156.0" text="Ingresar">
         <font>
            <Font name="Century Gothic Bold" size="24.0" />
         </font>
      </Button>
      <PasswordField fx:id="txtContrasenaLogin" layoutX="52.0" layoutY="248.0" onAction="#enter" prefHeight="35.0" prefWidth="280.0" promptText="Ingrese su contraseña" />
      <Label fx:id="txtadvertencia" alignment="CENTER_RIGHT" layoutX="189.0" layoutY="182.0" prefHeight="17.0" prefWidth="143.0" textAlignment="RIGHT" textFill="#f80909">
         <font>
            <Font name="Century Gothic Bold" size="11.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
